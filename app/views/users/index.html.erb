<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>
<table>
	<tr>
		<td>email</td>
		<td>account name </td>
		<td>password</td>
		<td>location</td>
		<td>age</td>
	</tr>
	<% @users.each do |user|%>
	<tr>
		<td><%= user.email %></td>
		<td><%= user.account_name %></td>
		<td><%= user.password %></td>
		<td><%= user.location %></td>
		<td><%= user.age %></td>
	</tr>
	<% end %>
</table>