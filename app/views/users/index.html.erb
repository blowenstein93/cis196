<table>
	<tr>
		<td>account name</td>
		<td>email</td>
		<td>password</td>
		<td>location</td>
		<td>age</td>
	</tr>
	<% @users.each do |user|%>
	<tr>
		<td><%= link_to user.account_name, user_path(user.id) %></td>
		<td><%= user.email %></td>
		<td><%= user.password %></td>
		<td><%= user.location %></td>
		<td><%= user.age %></td>
		<td><%= button_to "delete user", user, method: :delete %></td>
		<td><%= link_to "edit user", edit_user_path(user.id) %></td>
	</tr>
	<% end %>
</table>