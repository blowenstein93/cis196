<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>


<%=form_for @user do|f| %>
	<div class="field">
		<p>account name</p>
		<%=f.text_field :account_name%>
	</div>
	<div class="field">
		<p>email</p>
		<%=f.text_field :email%>
	</div>
	<div class="field">
		<p>password</p>
		<%=f.text_field :password%>
	</div>
	<div class="field">
		<p>location</p>
		<%=f.text_field :location%>
	</div>
	<div class="field">
		<p>age</p>
		<%=f.text_field :age%>
	</div>

	<div class="actions">
		<%=f.submit%>
	</div>
<%end%>